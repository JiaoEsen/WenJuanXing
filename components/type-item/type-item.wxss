@import "../../app.wxss";

.type_container {
  box-sizing: border-box;
  width: 100%;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  padding: 0 30rpx;
  margin-top: 60rpx;
}

.type_wrapper {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  width: 330rpx;
  height: 300rpx;
  border-radius: 10rpx;
  margin-bottom: 30rpx;
  background: #fff;
}

.type_wrapper .type_img {
  width: 83rpx;
  height: 86rpx;
}

.type_wrapper .type_into_text {
  line-height: 70rpx;
}