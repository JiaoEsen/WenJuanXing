/* components/tab-bar/tab-bar.wxss */
@import '../../font/iconfont.wxss';
.tabBar-container {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  display: flex;
  justify-content: space-between;
  font-size: 22rpx;
  background: #fff;
}


.tabBar-container>view {
  width: calc(100%/4);
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 10rpx 0;
}